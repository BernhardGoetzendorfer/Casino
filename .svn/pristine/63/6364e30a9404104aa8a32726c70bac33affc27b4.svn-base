<Window x:Class="Roulette0._0._8.InfoFensterRoulette"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Roulette0._0._8"
        mc:Ignorable="d"
        AllowsTransparency="True"
        WindowStyle="None"
        WindowState="Maximized"
        Title="InfoFensterRoulette" Height="1024" Width="1280" MouseLeftButtonDown="Window_MouseLeftButtonDown">
    <Window.Background>
        <SolidColorBrush Opacity="0.8" Color="Black"/>
    </Window.Background>
    <Canvas>
        <Label x:Name="lblPic1" Content="Schritt 1: Jeton Wert auswählen." Canvas.Left="26" Canvas.Top="476" Height="50" Width="294" FontSize="20" Foreground="White" MouseLeftButtonDown="lblPic1_MouseLeftButtonDown"/>
        <Label x:Name="lblPic2" Content="Schritt 2: Auf Zahl, gerade/ungerade oder rot/schwarz setzen." Canvas.Left="660" Canvas.Top="476" Height="50" Width="560" FontSize="20" Foreground="White"/>
        <Label x:Name="lblPic3" Content="Schritt 3: Zum Starten der Runde Start-Button klicken." Canvas.Left="26" Canvas.Top="964" Height="50" Width="489" FontSize="20" Foreground="White"/>
        <Label x:Name="lblPic4" Content="Schritt 4: Anzeigen von Gewinnzahl und Punkte." Canvas.Left="660" Canvas.Top="964" Height="50" Width="450" FontSize="20" Foreground="White"/>
        <Image x:Name="schritt1_jpg" Height="440" Source="Bilder/schritt1.jpg" Stretch="Fill" Canvas.Top="36" Width="610" Canvas.Left="26"/>
        <Image x:Name="schritt2_jpg" Height="440" Canvas.Left="660" Source="Bilder/schritt2.jpg" Stretch="Fill" Canvas.Top="36" Width="610"/>
        <Image x:Name="schritt3_jpg" Height="440" Canvas.Left="26" Source="Bilder/schritt3.jpg" Stretch="Fill" Canvas.Top="525" Width="610"/>
        <Image x:Name="schritt4_jpg" Height="440" Canvas.Left="660" Source="Bilder/schritt4.jpg" Stretch="Fill" Canvas.Top="525" Width="610"/>


    </Canvas>
</Window>