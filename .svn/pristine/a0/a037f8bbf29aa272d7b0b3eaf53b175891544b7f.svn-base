<!--BEGIN FULLSCREEN ############################################################-->
<Window x:Class="Roulette0._0._8.GewinnAusgabe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Roulette0._0._8"
        mc:Ignorable="d"
        AllowsTransparency="True"
        WindowStyle="None"
        WindowState="Maximized"
        Title="GewinnAusgabe" Height="1024" Width="1280" MouseLeftButtonDown="Window_MouseLeftButtonDown">
    <Window.Background>
        <SolidColorBrush Opacity="0.5" Color="Black"/>
    </Window.Background>
    <Grid>
        <Image x:Name="superGewinn" Source="Bilder/supergewinn.png" HorizontalAlignment="Left" Height="569" VerticalAlignment="Top" Width="746" Margin="285,233,0,0" Visibility="Hidden"/>
        <Image x:Name="megaGewinn" Source="Bilder/Megagewinn.png" HorizontalAlignment="Left" Height="569" VerticalAlignment="Top" Width="746" Margin="285,236,0,0" Visibility="Hidden"/>
        <Image x:Name="monsterGewinn" Source="Bilder/monstergewinn.png" HorizontalAlignment="Left" Height="569" VerticalAlignment="Top" Width="746" Margin="285,236,0,0" Visibility="Hidden"/>
        <Image x:Name="hoechstGewinn" Source="Bilder/hoechstgewinn.png" HorizontalAlignment="Left" Height="569" VerticalAlignment="Top" Width="746" Margin="285,236,0,0" Visibility="Hidden"/>
        <Label x:Name="lblpunkte" Content="Punkte:" HorizontalAlignment="Left" Margin="414,616,0,0" VerticalAlignment="Top" FontSize="64" Width="387" FontWeight="Bold" Background="{x:Null}" BorderBrush="#FF020202" BorderThickness="0" FontFamily="Bauhaus 93" Visibility="Hidden">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFCF1F0D"/>
                    <GradientStop Color="#FFFDF208" Offset="1"/>
                </LinearGradientBrush>
            </Label.Foreground>
        </Label>
        <Label x:Name="lblPunkteGewinnAnzeige" Content=" " HorizontalAlignment="Left" Margin="689,616,0,0" VerticalAlignment="Top" FontSize="64" Width="387" FontWeight="Bold" Background="{x:Null}" FontFamily="Bauhaus 93">
            <Label.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFCF1F0D" Offset="0"/>
                    <GradientStop Color="#FFFDF208" Offset="1"/>
                </LinearGradientBrush>
            </Label.Foreground>
        </Label>


    </Grid>
    <!--END FULLSCREEN ############################################################-->
